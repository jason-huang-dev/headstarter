{
    "builds": [
        {
            "src": "calendar_app/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" , "maxDuration": "60" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "calendar_app/wsgi.py"
        }
    ]
}
